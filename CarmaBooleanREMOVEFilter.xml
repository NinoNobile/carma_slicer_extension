<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>Cardiac MRI Toolkit</category>
  <title>Boolean REMOVE Filter</title>
  <description><![CDATA[Subtracts two segmentations (label maps). Data must be loaded as label maps in order to be used as inputs to this filter.]]></description>
  <version>0.1.0</version>
  <documentation-url>http://www.slicer.org/slicerWiki/index.php/Documentation/Nightly/Modules/SlicerModuleBooleanRemove</documentation-url>
  <license>Slicer</license><license>Slicer</license>
  <contributor>Salma Bengali(CARMA), Alan Morris(CARMA), Josh Cates(CARMA), Rob MacLeod(CARMA)</contributor>
  <acknowledgements><![CDATA[This work was supported by ...]]></acknowledgements>
  <parameters>
    <label>IO</label>
    <description><![CDATA[Input/output parameters]]></description>
    <image type = "label">
      <name>inputVolume1</name>
      <label>Input Volume 1</label>
      <channel>input</channel>
      <index>0</index>
      <description><![CDATA[Input volume 1]]></description>
    </image>
    <image type = "label">
      <name>inputVolume2</name>
      <label>Input Volume 2</label>
      <channel>input</channel>
      <index>1</index>
      <description><![CDATA[Input volume 2]]></description>
    </image>
    <image type = "label">
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>2</index>
      <description><![CDATA[Input Volume 1 - Input Volume 2]]></description>
    </image>
  </parameters>
</executable>
