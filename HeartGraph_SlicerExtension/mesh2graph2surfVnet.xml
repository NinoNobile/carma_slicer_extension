<?xml version="1.0" encoding="utf-8"?>
<executable>
    <category>CARMA</category> 
    <title>Mesh to Graph to SurfVnet</title>
    <description>This module implements an algorithm to automatically segment out the Left Atrium from an LGE-MRI image volume.</description>
    <version>0.1.0</version>
    <documentation-url></documentation-url>
    <license>Slicer</license>
    <contributor>Gopal Veni, Salma Bengali, Greg Gardner, Alan Morris, Josh Cates, Rob MacLeod, Ross Whitaker</contributor>
    <acknowledgements><![CDATA[This work was supported by ...]]></acknowledgements>
    <parameters>
        <label>IO</label>
        <description>Input/output parameters</description>
        <image>
            <name>inputImage</name>
            <label>Input Image</label>
            <index>0</index>
            <channel>input</channel>
            <description><![CDATA[Cardiac LGE-MRI image]]></description>
        </image>
        <integer>
            <name>deltaS</name>
            <label>Smoothness Parameter</label>
            <index>1</index>
            <channel>input</channel>
            <description><![CDATA[Smoothness constraint for a surface]]></description>
        </integer>
	<integer>
            <name>MdlOption</name>
            <label>Model selection Parameter</label>
            <index>2</index>
            <channel>input</channel>
            <description><![CDATA[Selection of model]]></description>
        </integer>
        <image type="label">
            <name>outputImage</name>
            <label>Output Image</label>
            <index>3</index>
            <channel>output</channel>
            <description><![CDATA[The segmented output image volume]]></description>
        </image>
        <directory>
            <name>inputDataDirectory</name>
            <label>Input Data Directory</label>
            <index>4</index>
            <channel>input</channel>
            <description><![CDATA[The directory from which to import data files]]></description>
            <!--<default>/Users/salmabengali/data/1/</default> -->
        </directory> 
	 <file>
            <name>epiMesh</name>
            <label>Epi Mesh</label>
            <index>5</index>
            <channel>output</channel>
            <description><![CDATA[The epi mesh]]></description>
        </file>
        <file>
            <name>endoMesh</name>
            <label>Endo Mesh</label>
            <index>6</index>
            <channel>output</channel>
            <description><![CDATA[The endo mesh]]></description>
        </file>
	 <float>
            <name>row</name>
            <label>Row</label>
            <index>7</index>
            <channel>input</channel>
            <description><![CDATA[Center of mass row]]></description>
        </float>
	 <float>
            <name>column</name>
            <label>Column</label>
            <index>8</index>
            <channel>input</channel>
            <description><![CDATA[Center of mass column]]></description>
        </float>
	 <float>
            <name>slice</name>
            <label>Slice</label>
            <index>9</index>
            <channel>input</channel>
            <description><![CDATA[Center of mass Slice]]></description>
        </float>
       <!--<file multiple="true">
            <name>args</name>
            <index>3</index>
            <description>The directory from which to import data files</description>
       </file> -->
    </parameters>
</executable>